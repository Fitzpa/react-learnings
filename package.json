{
  "name": "react-learnings",
  "version": "1.0.0",
  "description": "This is a repo of things you can do in React that is intended for me to better learn React and have a reference to look back on in the future.",
  "main": "index.js",
  "scripts": {
    "build": "parcel build --public-url ./dist/ src/index.html",
    "clear-build-cache": "rm -rf .cache/ dist/",
    "format": "prettier \"src/**/*.{js,html}\" --write",
    "lint": "eslint \"src/**/*.{js,jsx}\" --quiet",
    "start": "npm run build && babel-node server/index.js",
    "dev": "parcel src/index.html",
    "dev:mock": "cross-env PET_MOCK=mock npm run dev",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Fitzpa/react-learnings.git"
  },
  "author": "Louie Fitzpatrick -- Fitzpa",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Fitzpa/react-learnings/issues"
  },
  "homepage": "https://github.com/Fitzpa/react-learnings#readme",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "jest": "^25.5.3",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.0.5"
  },
  "dependencies": {
    "@babel/node": "^7.8.7",
    "@emotion/babel-preset-css-prop": "^10.0.27",
    "@emotion/core": "^10.0.28",
    "@frontendmasters/pet": "^1.0.3",
    "@reach/router": "^1.3.3",
    "babel-cli": "^6.26.0",
    "express": "^4.17.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "browserslist": [
    "last 2 Chrome versions"
  ],
  "comments": {
    "babel/node": "Allows us to use all of our node code through babel as well"
  }
}
